    class GalleryItem:
        def __init__(self, name, image_path, categories="Everything", shown_as=None, locked="locked", tooltip="", thumbnail=False):
            self.name = name
            
            # Handle legacy list format by taking first item
            if isinstance(image_path, (list, tuple)):
                image_path = image_path[0]
                
            # Store full path
            self.full_path = image_path
            if not self.full_path.startswith("images/"):
                self.full_path = f"images/{image_path}"
                
            # Handle file extensions
            if not any(self.full_path.endswith(ext) for ext in ['.webp', '.png', '.jpg', '.jpeg']):
                self.full_path = f"{self.full_path}.webp"
                
            # Extract display name without path or extension
            self.display_name = image_path
            if "/" in self.display_name:
                self.display_name = self.display_name.split("/")[-1]
            self.display_name = self.display_name.rsplit('.', 1)[0]
            
            # Store the name used in show statements if provided
            self.shown_as = shown_as if shown_as else self.display_name
            
            if isinstance(categories, str):
                self.categories = [categories]
            elif isinstance(categories, list):
                self.categories = categories
            else:
                self.categories = ["Everything"]
            
            self.locked = locked
            self.tooltip = tooltip if tooltip else name
            self.thumbnail = thumbnail
            self.refresh_lock()

        def refresh_lock(self):
            """Check if the image has been seen"""
            try:
                # Check if shown_as has been seen
                if renpy.seen_image(self.shown_as):
                    self.is_locked = False
                    return
                
                elif hasattr(persistent, 'unlocked_gallery_items') and self.display_name in persistent.unlocked_gallery_items:
                    self.is_locked = False
                else:
                    # Fallback to checking display_name if shown_as hasn't been seen
                    self.is_locked = not renpy.seen_image(self.display_name)
            except:
                # If there's any error, lock the item
                self.is_locked = True
