#gallery background
image gray = "#777"

style checkbox_text is text:
    color "#fff"
    size 16
    xalign 0.0




screen gallery_category_select():
    tag menu
    
    default categories = get_unique_categories()
    
    add "black"
    text "Gallery" size 40 xalign 0.5 color "#fff"

    hbox:
        xalign 0.06
        yalign 0.01
        spacing 10
        style "checkbox_text"
        button:
            ysize 45
            xsize 250
            text ("Show Unlocked First"):
                style "checkbox_text"
                color "#fff"
                yalign 0.5 xpos 18
            add Transform("gui/checkbox_[persistent.show_only_unlocked].png"):
                xalign 0.95 yalign 0.5 zoom 0.8
            action ToggleVariable("persistent.show_only_unlocked")
            style "checkbox_text"
            background "#242424"
            hover_background "#fffa"
            hovered Show("displayTextScreen", displayText="When enabled, selecting a category shows all unlocked images first")
            unhovered Hide("displayTextScreen")
            hover_sound "audio/soun_fx/select2.opus"
            activate_sound "audio/soun_fx/select4.opus"
    hbox:
        xalign 0.887
        yalign 0.01
        spacing 10
        style "checkbox_text"
        if persistent.current_activation == "Free" or persistent.current_activation == "Chunin":
            button:
                ysize 45
                xsize 250
                text ("Gallery Unlocker"):
                    style "checkbox_text"
                    color "#fff"
                    yalign 0.5 xpos 18
                add Transform("gui/checkbox_False.png"):
                    xalign 0.95 yalign 0.5 zoom 0.8
                action Confirm(
                    "This feature is only available for supporters of at least {color=#9b59b6}Jonin+{/color} rank.{p} Would you like to check out the supporter's benefits now?",
                    yes=ToggleScreen("subscription_tiers"),
                    no=None
                )
                hovered Show("displayTextScreen", displayText="When enabled, allows unlocking entries by simply clicking on them")
                unhovered Hide("displayTextScreen")
                style "checkbox_text"
                background "#FFD700"
                hover_background "#fffa"
                hover_sound "audio/soun_fx/select2.opus"
                activate_sound "audio/soun_fx/select4.opus"
        else:
            button:
                ysize 45
                xsize 250
                text ("Gallery Unlocker"):
                    style "checkbox_text"
                    color "#fff"
                    yalign 0.5 xpos 18
                add Transform("gui/checkbox_True.png"):
                    xalign 0.95 yalign 0.5 zoom 0.8
                action NullAction()
                hovered Show("displayTextScreen", displayText="Gallery Unlocker is enabled. Click a gallery entry to unlock it!")
                unhovered Hide("displayTextScreen")
                style "checkbox_text"
                background "#242424"
                hover_background "#FFD700"
                hover_sound "audio/soun_fx/select2.opus"
                activate_sound "audio/soun_fx/select4.opus"



    frame:
        background "#0008"
        margin (50, 50)
        
        viewport:
            draggable True
            mousewheel True
            scrollbars "vertical"
            ysize 700
            
            vbox:
                spacing 20
                xalign 0.5
                yalign 0.5
                
                
                
                # Calculate number of rows needed (ceiling division)
                $ rows = (len(categories) + 3) // 4
                
                grid 4 rows:
                    spacing 20
                    xalign 0.5
                    
                    for category in categories:
                        frame:
                            background "#242424"
                            padding (2, 2)
                            margin (5, 5)
                            
                            fixed:
                                xysize (250, 300)
                                
                                if category == "Everything":
                                    imagebutton:
                                        idle Transform("images/characters/kurama/kurama2.webp", size=(250, 300), alpha=0.8)
                                        hover Transform("images/characters/kurama/kurama2.webp", size=(250, 302), alpha=1.0)
                                        action [SetVariable("gallery_page", 0), SetVariable("current_category", category), Show("gallery")]
                                        hover_sound "audio/soun_fx/select2.opus"
                                        activate_sound "audio/soun_fx/select4.opus"

                                else:
                                    $ thumbnail = get_category_thumbnail(category)
                                    if thumbnail:
                                        imagebutton:
                                            idle Transform(thumbnail, size=(250, 300), alpha=0.8)
                                            hover Transform(thumbnail, size=(250, 302), alpha=1.0)
                                            action [SetVariable("gallery_page", 0), SetVariable("current_category", category), Show("gallery")]
                                            hover_sound "audio/soun_fx/select2.opus"
                                            activate_sound "audio/soun_fx/select4.opus"
                                
                                text category:
                                    xalign 0.5
                                    yalign 0.5
                                    size 24
                                    color "#fff"
                                    outlines [(2, "#000", 0, 0)]

                                $ unlocked, total = get_category_counts(category)
                                text f"({unlocked}/{total})":
                                    xalign 0.5
                                    yalign 0.9  # Position below category name
                                    size 20
                                    color "#fff"
                                    outlines [(2, "#000", 0, 0)]

    textbutton "{color=#000}Return{/color}" style "powerup_button":
        action Return()
        xalign 0.5
        yalign 0.98
        background "#fff8"
        hover_background "#fffa"
        hover_sound "audio/soun_fx/select2.opus"
        activate_sound "audio/soun_fx/select4.opus"

