    def get_filtered_items(category, show_unlocked_first=False):
        """Get filtered items based on category and sort by unlock status"""
        if category == "Everything":
            items = gallery_items
        else:
            items = [item for item in gallery_items if category in item.categories]
        
        # Update lock status for all items
        for item in items:
            item.refresh_lock()
            
        # Sort items by unlock status if needed
        if show_unlocked_first:
            # Create two lists and then combine them
            unlocked = [item for item in items if not item.is_locked]
            locked = [item for item in items if item.is_locked]
            items = unlocked + locked
            
        return items

    def get_category_thumbnail(category):
        """Find first image marked as thumbnail for this category"""
        for item in gallery_items:
            if category in item.categories and item.thumbnail:
                # Return the full path instead of accessing images list
                return item.full_path
        return None

    def get_unique_categories():
        category_order = ["Everything", "Boruto", "Hinata", "Himawari", "Ino", "Tsunade", "Tenten", "Obo", "Yoruichi", "Madame", "Sakura", "Sarada", "Kushina" "Uncategorized"]  # Added "Uncategorized"
        categories = set()
        for item in gallery_items:
            categories.update(item.categories)

        # Sort based on the predefined order, putting any undefined categories at the end
        def custom_sort(x):
            try:
                return category_order.index(x)
            except ValueError:
                return len(category_order)  # Just return the length instead of trying to find "Uncategorized"
        return sorted(list(categories), key=custom_sort)


    def get_category_counts(category):
        # Refresh lock status for all items first
        for item in gallery_items:
            item.refresh_lock()
            
        if category == "Everything":
            total = len(gallery_items)
            unlocked = sum(1 for item in gallery_items if not item.is_locked)
        else:
            total = sum(1 for item in gallery_items if category in item.categories)
            unlocked = sum(1 for item in gallery_items 
                        if category in item.categories and not item.is_locked)
        return unlocked, total



    def extract_display_name(image_path):
        """Extract display name from image path, handling different formats"""
        # Remove file extension
        name = image_path.replace(".webp", "").replace(".png", "").replace(".jpg", "")
        # If it's a full path, get the last part
        if "/" in name:
            name = name.split("/")[-1]
        # Remove 'images/' prefix if present
        if name.startswith("images/"):
            name = name[7:]
        return name

